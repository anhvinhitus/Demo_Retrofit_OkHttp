<resources>

    <style name="CardViewStyle" parent="android:Widget.TextView">
        <item name="cardCornerRadius">@dimen/cardview_corner_radius</item>
        <item name="cardElevation">@dimen/cardview_elevation</item>
        <item name="cardPreventCornerOverlap">false</item>
        <item name="cardUseCompatPadding">true</item>
    </style>

    <style name="Toolbar.TitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/textsize.large</item>
    </style>
</resources>
